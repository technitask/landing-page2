<body>
  <header id="header"> 
 </header> 

  <div id="index-banner" class="parallax-container">
    <div class="section no-pad-bot">
      <div class="container">
        <br><br>
        <h1 class="header center teal-text text-lighten-2"><img src="images/technitasklogo10.png" height="52%" width="52%" alt="Unsplashed logo"></h1>
        <div class="row center">
          <h5 class="header col s12 light">Get in Touch</h5>
          <p>To inquire about any of our products or services please contact us at the e-mail addresses below or submit your query via the form.</p>
          <h4>info@technitask.com</h4>
        </div>
        <!-- <div class="row center">
          <a href="http://materializecss.com/getting-started.html" id="download-button" class="btn-large waves-effect waves-light light-blue darken-1">Contact Us</a>
        </div> -->
        <br><br>

      </div>
    </div>
  </div>

    
  <div class="container">
      <div class="row">
        <form class="col s8">
          <div class="row">
            <div class="input-field col s4">
              <i class="material-icons prefix">account_circle</i>
              <input id="icon_prefix" type="text" class="validate">
              <label for="icon_prefix">First </label>
            </div>
            <div class="input-field col s4">
              <i class="material-icons prefix">account_circle</i>
              <input id="icon_prefix" type="text" class="validate">
              <label for="icon_prefix">Last </label>
            </div>
              <div class="input-field col s8">
                <i class="material-icons prefix">email</i>
                <input id="email" type="email" class="validate">
                <label for="email" data-error="wrong" data-success="right">Email</label>
              </div>
           <div class="input-field col s8">
              <i class="material-icons prefix">subject</i>
              <input id="icon_prefix" type="text" class="validate">
              <label for="icon_prefix">Subject</label>
            </div> 

          <div class="input-field col s8">
            <i class="material-icons prefix">mode_edit</i>
            <textarea id="icon_prefix2" class="materialize-textarea"></textarea>
            <label for="icon_prefix2">Message</label>
          </div>
      </div>
     
        <button class="btn waves-effect waves-light black" type="submit" name="action">Submit
          <i class="material-icons right">send</i>
        </button>
    </form>
      <div class="col s4">
            <h5>
              Address
            </h5>
            <p>
              130 King Street West, Suite 1800, Toronto, Ontario, M5X 1E3
            </p>
            <h5>
              Phone #
            </h5>
            <p>
              1-647-660-1020
            </p>
            <h5>
              Email
            </h5>
            <p>
              info@technitask.com
            </p>
      </div>
    </div>
  </div>
   

  
<footer id="footer"> 
  </footer> 


